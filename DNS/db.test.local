;; db.test.local
;; Forwardlookupzone für domainname
;;
$TTL 2D
@       IN      SOA     DNSServer.test.local. (
                        2018032201      ; Serial
                                8H      ; Refresh
                                2H      ; Retry
                                4W      ; Expire
                                3H )    ; NX (TTL Negativ Cache)
;
@                               IN      NS      DNSServer.test.local.
                                IN      A       192.168.56.100
	

DNSServer          		IN      A       192.168.56.100
localhost                       IN      A       127.0.0.1

